#include <stdio.h>
#include <string.h>

void removeAdjacentDuplicates(char *str) {
    int top = -1;  
    int n = strlen(str);
    char stack[n + 1];  

    for (int i = 0; i < n; i++) {
        if (top >= 0 && stack[top] == str[i]) {
            top--;  
        } else {
            stack[++top] = str[i];  
        }
    }

    stack[top + 1] = '\0'; 

    printf("%s\n", stack);
}

int main() {
    char str[101];  
    scanf("%s", str);
    removeAdjacentDuplicates(str);
    return 0;
}
